<div class=main-container>
  <div class=header>
    <h1><%= @group.name %></h1>
  </div>

  <div class=body-container>
    <div class=body-left>
      <div class=box>
          <form action="/members/shows" >
            <%= select_tag(:mem_id, options_for_select(@member_options)) %>
            <input type="submit">
          </form>
          <%= link_to "Add member", new_group_member_path(@group) %>
      </div>

      <div class=box>
        Some things
      </div>
      <div class=square>
      </div>

    </div>
    <div class=body-right>
      <div class=box>
        Some more things
      </div>
    </div>
  </div>

  <div class=footer>
    <div class=footer-text>
      interesting things here.
    </div>
  </div>
</div>
